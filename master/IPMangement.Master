        <script type="text/javascript">
            function runEffect2() {
                // get effect type from
                var selectedEffect = "drop";
                // run the effect
                $("#effect2").show(selectedEffect, 0, callback2);
            };

            //callback function to bring a hidden box back
            function callback2() {
                setTimeout(function () {
                    $("#effect2:visible").removeAttr("style").fadeOut();
                }, 3000);
            };

            $("#effect2").hide();
        
