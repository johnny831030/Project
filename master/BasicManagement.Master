    <script type="text/javascript">
        setInterval("protectTimeOut()", 600000);
        function protectTimeOut() {
            document.getElementById('refresh').contentDocument.location.reload(true);
        }
        $(function () {
            $('#top').click(function () {
                $('html,body').animate({ scrollTop: '0px' }, 500);
            });

            $('#ft').click(function () {
                $('html,body').animate({ scrollTop: $('#footer1').offset().top }, 500);
            });
            $("#Menu1 ul.level1").css("display", "block"); //"initial"
        });

        function warning() {
            document.getElementById('<%= lblShowErr.ClientID%>').innerText = "施工中，敬請見諒";
            runEffect1();
        }

        function runEffect1() {
            // get effect type from 
            var selectedEffect = "drop";
            // run the effect
            $("#effect3").show(selectedEffect, 0, callback1);
        };

        //callback function to bring a hidden box back
        function callback1() {
            setTimeout(function () {
                $("#effect3:visible").removeAttr("style").fadeOut();
            }, 3000);
        };

        $("#effect3").hide();
    </script>
