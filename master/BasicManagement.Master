    <style type="text/css">
        /** 滑動選單 **/
        #top, #ft {
            position: relative;
            cursor: pointer;
            height: 40px;
            width: 25px;
        }

        #top {
            background: url(/Image/WebImage/roll.png) no-repeat;
        }

        #ft {
            background: url(/Image/WebImage/roll.png) no-repeat 0 -80px;
        }

        #roll {
            display: block;
            width: 25px;
            margin-right: -600px;
            position: fixed;
            right: 50%;
            top: 50%;
        }

        #effect, #effect1 {
            display: none;
            top: 40%;
            right: 50%;
            position: fixed;
        }

        .toggler, .toggler1 {
            right: 100%;
        }

        .toggler3 {
            position: fixed;
            top: 180px;
            right: 350px;
            width: 200px;
            height: 20px;
        }

        #effect3 {
            position: absolute;
            top: 180px;
            right: 350px;
            width: 240px;
            /*height: 50px;*/
            padding: 0.4em;
            position: relative;
        }
        
            #effect3 p {
                height: 100%;
                margin: 0;
                padding: 0.2em;
                text-align: center;
            }

        .ui-widget-content1 {
            border: 1px solid #aaaaaa;
            background: #FF3333;
            color: #FFFFFF;
        }
    </style>

</head>

